{
    "name": "kuakling/yii2-materialize",
    "description": "The Materialize extension for Yii framework 2",
    "keywords": ["yii2", "materialize"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/kuakling/yii2-materialize/issues",
        "source": "https://github.com/kuakling/yii2-materialize"
    },
    "homepage": "https://github.com/kuakling/yii2-materialize",
    "authors": [
        {
            "name": "Surakit Choodet",
            "email": "kuakling@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "bower-asset/materialize": "0.97.*"
    },
    "autoload": {
        "psr-4": {
            "kuakling\\materialize\\": "/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
